generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  user String @unique
  password String
  email String @unique
  cpf String @unique
  birth_date DateTime?
  plan String?
}